@model Permission
@{
    ViewData["Title"] = "Details";
}

<!-- Page-header start -->
<div class="page-header">
    <div class="row align-items-end">
        <div class="col-lg-8">

        </div>
        <div class="col-lg-4">
        </div>
    </div>
</div>
<!-- Page-header end -->
<!-- Page-body start -->
<div class="page-body">
    <!-- Basic table card start -->
    <div class="card">
        <div class="card-block">
            <h4 class="sub-title">Basic Inputs</h4>
            <div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Name">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Name
                        </p>
                    </div>
                </div>
                 <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Surname">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Surname
                        </p>
                    </div>
                </div>
                 <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Reason">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Reason
                        </p>
                    </div>
                </div>
                 <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Date">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Date
                        </p>
                    </div>
                </div>
                 <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Duration">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Duration
                        </p>
                    </div>
                </div>
                 <div class="form-group row">
                    <label class="col-sm-2 col-form-label" asp-for="Profession">
                    </label>
                    <div class="col-sm-10">
                        <p class="form-control">
                            @Model.Profession
                        </p>
                    </div>
                </div>
               
                <div class="form-group row">
                    <div class="col-12 d-flex justify-content-end">
                        <a class="btn btn-info" asp-action="index">Go Back</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Page-body end -->


